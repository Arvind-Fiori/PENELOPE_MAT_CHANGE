<mvc:View
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:t="sap.ui.core"
    xmlns:f="sap.ui.layout.form"
    controllerName="arvind.pp.penelope.matchange.Controller.DMS"
>
    <Page
        title="Product Image"
        xmlns:core="sap.ui.core"
        showNavButton="true"
        navButtonPress="onBack"
    >
     <customHeader>
            <Bar>
                <contentLeft>
                <!-- <Button icon="sap-icon://nav-back" id="btnback"></Button> -->
                </contentLeft>
                <contentMiddle>
                    <HBox
                        class="header"
                        width="100%"
                    >
                    <!-- <Button icon="sap-icon://nav-back" id="btnbackDMS"  press ="onBack"></Button> -->
                    <!-- <Button text="" id="btnback"  press ="onBack"></Button> -->
                        <Label id="LBLIMAGE" class="LBLIMAGE"
                            text="Material Image"
                            width="100%"
                            
                            
                        />
                    </HBox>
                </contentMiddle>
            </Bar>
        </customHeader>
<ScrollContainer  width="100%" horizontal="true" vertical="true" focusable="true">
        <Image id="DMS" />
</ScrollContainer>        

        <footer>
            <Toolbar>
                <ToolbarSpacer />
                <Button
                    id="btnBack"
                    text="Back"
                    press="onBack"
                    type="Accept"
                    icon="sap-icon://nav-back"
                />
            </Toolbar>
        </footer>
    </Page>
</mvc:View>
